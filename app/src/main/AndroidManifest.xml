<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.finalwork">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinalWork"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".NewsDetailActivity"
            android:exported="false" />
        <activity
            android:name=".FragmentThree"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".FragmentTwo"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".FragmentOne"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".OwnSetActivity"
            android:exported="false" />
        <activity
            android:name=".RateListActivity"
            android:exported="true" />
        <activity
            android:name=".RateActivity"
            android:exported="false" />
        <activity
            android:name=".RateNewsActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignupActivity"
